{
  "outputs": [
    {
      "class": "io.github.aemogie.timble.utils.logging.ConsoleLoggerOutput",
      "level": "all",
      "pattern": "[!!hour:!!min:!!sec] [!!thread/!!level] !!msg"
    }, {
      "class": "io.github.aemogie.timble.utils.logging.FileLoggerOutput",
      "level": "all",
      "pattern": "[!!hour:!!min:!!sec] [!!thread/!!level] !!msg",
      "config": {
        "path": "logs/latest.log"
      }
    }, {
      "class": "io.github.aemogie.timble.utils.logging.FileLoggerOutput",
      "level": "error",
      "pattern": "[!!hour:!!min:!!sec] [!!thread] !!msg",
      "config": {
        "path": "logs/errors.log"
      }
    }
  ]
}